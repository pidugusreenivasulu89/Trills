{
  "expo": {
    "name": "Trills",
    "slug": "trills",
    "scheme": "trillsauth",
    "version": "2.0.0",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4B184C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trills.socialvibe"
    },
    "android": {
      "package": "com.trills.socialvibe",
      "versionCode": 13,
      "permissions": [
        "CAMERA",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "!android.permission.AD_ID"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4B184C"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "sreeni89s-organization",
    "extra": {
      "eas": {
        "projectId": "8706a40c-88c5-439e-8bdc-858449b49349"
      }
    }
  }
}